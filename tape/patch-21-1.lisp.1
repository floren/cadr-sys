#10R TAPE#: 
(COMPILER-LET ((*PACKAGE* (PKG-FIND-PACKAGE "TAPE")))
  (COMPILER::PATCH-SOURCE-FILE NIL

;;;In case the old name "TAR-FORMAT" got loaded:

(setq *tape-format-alist*
      (delete "tar-format" *tape-format-alist*
	      :test #'string-equal
	      :key #'car))
