(DEFSITE :MIT
  ;; OZ is where the sources are
  (:SYS-HOST "OZ")
  ;; SYS: translations to use
  (:SYS-HOST-TRANSLATION-ALIST '(("FC" . FC-SYS-PATHNAME-TRANSLATIONS)
				 ("OZ" . OZ-SYS-PATHNAME-TRANSLATIONS)
				 ("FS" . LM-SYS-PATHNAME-TRANSLATIONS)
