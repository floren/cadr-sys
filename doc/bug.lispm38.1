(DEFUN LOCAL-NETWORK-OF-TYPE (TYPE)
  ;; First try one declared to be at this site, then one named here, presumed
  ;; to be a network without a particular site.
  ;; Then one anywhere in the search rules.
  ;; Finally a network which the local host is on.
  (LET ((POSSIBILITIES (OR (SEARCHING-ONE-SITE ()
			     (FIND-OBJECTS-FROM-PROPERTY-LIST :NETWORK :TYPE TYPE
							      :SITE *LOCAL-SITE*))
			   (FIND-OBJECTS-FROM-PROPERTY-LIST :NETWORK :TYPE TYPE)
			   (LOOP FOR NETWORK IN *LOCAL-NETWORKS*
				 WHEN (SEND-IF-HANDLES NETWORK :NETWORK-TYPEP TYPE)
				   COLLECT NETWORK)
			   (FERROR "Could not find the local ~A network." TYPE))))
    ;; When there's more than one network, just include the ones that the local host
    ;; is on.
    (WHEN (REST1 POSSIBILITIES)
	(SETQ POSSIBILITIES
	      (LOOP FOR NETWORK IN POSSIBILITIES
		    WHEN (NS-MEMQ NETWORK *LOCAL-NETWORKS*)
		      COLLECT NETWORK)))
    (IF (REST1 POSSIBILITIES)
	(FERROR "There are several local ~A networks." TYPE)
	(FIRST POSSIBILITIES))))
