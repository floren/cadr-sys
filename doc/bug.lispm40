(defun tf (l1 l2 file)
  (with-open-file (str file :direction :output)
    (fresh-line str)
    (formatting-table (str)
      (formatting-column (str)
	(loop for x in l1 do (formatting-cell (str)(princ x str))))
      (formatting-column (str)
